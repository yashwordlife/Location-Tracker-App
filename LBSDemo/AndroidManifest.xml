<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lbsdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.lbsdemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.lbsdemo.Retrieve"
            android:label="@string/title_activity_retrieve" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.DateRect"
            android:label="@string/title_activity_date_rect" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.PlaceRet"
            android:label="@string/title_activity_place_ret" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.PlaceResult"
            android:label="@string/title_activity_place_result" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.DateResult"
            android:label="@string/title_activity_date_result" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.DateFinalResult"
            android:label="@string/title_activity_date_final_result" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.Final"
            android:label="@string/title_activity_final" >
        </activity>
        <activity
            android:name="com.example.lbsdemo.Last"
            android:label="@string/title_activity_last" >
        </activity>
    </application>

</manifest>